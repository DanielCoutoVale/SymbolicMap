    <!-- http://www.uppermodel.org/Transitivity.owl#F3 -->

    <owl:NamedIndividual rdf:about="&Transitivity;F3">
        <rdf:type rdf:resource="&Transitivity;Figure"/>
        <Transitivity:hasActor rdf:resource="&Transitivity;F3Actor"/>
        <Transitivity:hasProcess rdf:resource="&Transitivity;F3Process"/>
    </owl:NamedIndividual>
    


    <!-- http://www.uppermodel.org/Transitivity.owl#F3Actor -->

    <owl:NamedIndividual rdf:about="&Transitivity;F3Actor">
        <rdf:type rdf:resource="&Transitivity;Element"/>
    </owl:NamedIndividual>
    


    <!-- http://www.uppermodel.org/Transitivity.owl#F3Process -->

    <owl:NamedIndividual rdf:about="&Transitivity;F3Process">
        <rdf:type rdf:resource="&Transitivity;Running"/>
    </owl:NamedIndividual>
    


    <!-- http://www.uppermodel.org/Transitivity.owl#F4 -->

    <owl:NamedIndividual rdf:about="&Transitivity;F4">
        <rdf:type rdf:resource="&Transitivity;Figure"/>
        <Transitivity:hasActor rdf:resource="&Transitivity;F4Actor"/>
        <Transitivity:hasGoal rdf:resource="&Transitivity;F4Goal"/>
        <Transitivity:hasProcess rdf:resource="&Transitivity;F4Process"/>
