        <Transitivity:hasMedium rdf:resource="&Transitivity;F1Medium"/>
        <Transitivity:hasProcess rdf:resource="&Transitivity;F1Process"/>
    </owl:NamedIndividual>
    


    <!-- http://www.uppermodel.org/Transitivity.owl#F1Medium -->

    <owl:NamedIndividual rdf:about="&Transitivity;F1Medium">
        <rdf:type rdf:resource="&Transitivity;Element"/>
    </owl:NamedIndividual>
    


    <!-- http://www.uppermodel.org/Transitivity.owl#F1Process -->

    <owl:NamedIndividual rdf:about="&Transitivity;F1Process">
        <rdf:type rdf:resource="&Transitivity;Running"/>
    </owl:NamedIndividual>
    


    <!-- http://www.uppermodel.org/Transitivity.owl#F2 -->

    <owl:NamedIndividual rdf:about="&Transitivity;F2">
        <rdf:type rdf:resource="&Transitivity;Figure"/>
        <Transitivity:hasAgent rdf:resource="&Transitivity;F2Agent"/>
        <Transitivity:hasMedium rdf:resource="&Transitivity;F2Medium"/>
        <Transitivity:hasProcess rdf:resource="&Transitivity;F2Process"/>
    </owl:NamedIndividual>
    


